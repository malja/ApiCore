{
    "name": "apicore/core",
    "description": "PHP micro framework for creating REST API.",
    "keywords": ["framework", "rest", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Malčák",
            "email": "looorin@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/malja/PicORM"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.1",
        "altorouter/altorouter": "^1.0.0",
        "filp/whoops": "^2.2.0",
        "vanilla/garden-schema": "^v1.2.0",
        "zeptech/annotations": "^1.1.0",
        "symfony/var-dumper": "^v4",
        "picorm/picorm": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "malja\\ApiCore\\": "src/"
        }
    },
    "config": {
        "preferred-install": {
            "picorm/picorm": "source"
        },
        "*": "dist"
    }
}
